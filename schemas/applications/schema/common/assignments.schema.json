{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Assignments",
  "description": "A collection of named assignments with assigned values",
  "type": "object",
  "additionalProperties": false,
  "patternProperties": {
    "^[a-zA-Z0-9]([a-zA-Z0-9_.-]?[a-zA-Z0-9])*$": {
      "$ref": "assignment_target.schema.json"
    }
  }
}